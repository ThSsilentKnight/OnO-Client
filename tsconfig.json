{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "client/main",
    "rootDir": "client/src",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "strict": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["client/src/**/*"]
}
